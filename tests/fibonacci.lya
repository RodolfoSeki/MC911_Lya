fibo: proc (n int, g int loc);
  dcl h, i int;
  if n < 0 then
	 g = 1;
  else
	 fibo(n - 2, i);
	 fibo(n - 1, h);
    g = h + i;
  fi;
end;
dcl k int = 0;
fibo(-2, k);
print(k);
k=0;

fibo(-1, k);
print(k);
k=0;
fibo(0, k);
print(k);
k=0;
