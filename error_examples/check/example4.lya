/* example4: POINTERS*/

fibo: proc (n int, g int loc);
  dcl h int;
  if n < 0 then
    print(g);
    return;
  else
    h = g; fibo(n-1, h);
    g = h; fibo(n-2, g);
  fi;
  print(n,g);
end;

dcl k int = 0;
fibo(3,k); 



